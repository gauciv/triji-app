{
  "expo": {
    "name": "triji-app",
    "slug": "triji-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.triji.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.triji.app",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "com.google.android.c2dm.permission.RECEIVE"
      ]
    },
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#22e584",
      "androidMode": "default",
      "androidCollapsedTitle": "Triji"
    },
    "web": {},
    "plugins": [
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#22e584",
          "sounds": []
        }
      ]
    ]
  }
}
